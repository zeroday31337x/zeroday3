# ZeroDay3 Matching AI - Environment Variables

# Application
APP_NAME="ZeroDay3 Matching AI"
API_PREFIX="/api"

# CORS Configuration
# CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# Database
DATABASE_URL="sqlite+aiosqlite:///./zeroday3.db"

# Gemini AI Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY="your-gemini-api-key-here"
GEMINI_MODEL="gemini-pro"

# Admin Authentication
# Change this in production!
ADMIN_SECRET_KEY="your-secret-key-change-in-production"
ADMIN_ALGORITHM="HS256"
ADMIN_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Matching Algorithm Parameters
STRUCTURAL_LOGIC_WEIGHT=0.65
PRECISION_WEIGHT=0.35
